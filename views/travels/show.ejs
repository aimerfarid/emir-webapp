<% layout('layouts/travel-show-layout') -%>
<h1><%= travel.title %></h1>
<div>Location: <%= travel.location %></div>
<div><%= travel.description %></div>
<div class="">
  <% travel.images.forEach(function(image) { %>
    <img src="<%= image.url %>" alt="Travel Images" width="200px">
  <% }) %>
</div>

<div id='map'></div>

<div>
  <a href="/travels/<%= travel.id %>/edit">
    <button>Edit</button>
  </a>
</div>
<div class="">
  <form class="" action="/travels/<%= travel.id %>?_method=DELETE" method="POST">
    <input type="submit" value="Delete">
  </form>
</div>
